<view class="tab-bar">
  <view class="tab-bar-container">
  <block wx:for="{{list}}" wx:key="text">
    <view
      class="tab-item"
      bindtap="switchTab"
      data-path="{{item.pagePath}}"
      data-index="{{index}}"
    >
      <image class="tab-icon" src="{{selected === index ? item.selectedIconPath : item.iconPath}}"></image>
    </view>
  </block>
</view>
</view>
